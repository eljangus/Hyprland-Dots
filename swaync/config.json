{
    "$schema": "/etc/xdg/swaync/configSchema.json",
    "ignore-gtk-theme" : true,
    "cssPriority" : "user",
    "positionX": "right",
    "positionY": "top",
    "fit-to-screen": false,
    "control-center-margin-top": 23,
    "control-center-margin-bottom": 23,
    "control-center-margin-left": 23,
    "control-center-margin-right": 23,
    "notification-2fa-action": true,
    "timeout": 10,
    "timeout-low": 5,
    "timeout-critical": 0,
    "notification-window-width": 400,
    "notification-window-height": -1,
    "notification-grouping": true,
    "image-visibility": "when-available",
    "transition-time": 0,
    "hide-on-action": false,
    "text-empty": "Keine Benachrichtigungen",
    "control-center-width": 500,
    "control-center-height": 850,
    "widgets": [
      "title",
      "dnd",
      "notifications"
    ],
    "widget-config": {
      "notifications": {
        "vexpand": false
      },
    "title": {
      "text": "Benachrichtigungen",
      "button-text": "Löschen",
      "clear-all-button": true
    },
    "dnd": {
      "text": "Bitte Nicht Stören"
    }
    }
}
